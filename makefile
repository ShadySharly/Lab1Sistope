# - Makefile "maestro" que llama a los demas makefile. 
all: release

release:
	(cd pipeline; make)
	(cd reading; make)
	(cd convolution; make)
	(cd rectification; make)
	(cd pooling; make)
	(cd clasification; make)
	(cd writing; make)
	

clean:
	(cd pipeline; make clean)
	(cd reading; make clean)
	(cd convolution; make clean)
	(cd rectification; make clean)
	(cd pooling; make clean)
	(cd clasification; make clean)
	(cd writing; make clean)

#////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
